{
    "env": {
        "myDefaultIncludePath": ["${workspaceFolder}","${workspaceFolder}/DoOver/libs/**", "/usr/include/**"],
        "myCompilerPath": "/usr/bin/g++"
    },
    "configurations": [
        {
            "name": "Linux",
            "intelliSenseMode": "linux-gcc-x64",
            "includePath": [
                "${myDefaultIncludePath}"
            ],
            "defines": [],
            "compilerPath": "/usr/bin/g++",
            "cStandard": "gnu17",
            "cppStandard": "gnu++17",

            "configurationProvider": "ms-vscode.makefile-tools"
        }
    ],
    "version": 4
}